{
  "generateInto": "elm-gen/generated",
  "templatesFrom": "elm-gen/templates",
  "moduleBase": "Gen",
  "modules": {
    "Direction": {
      "Enum": {
        "sorts": ["Up", "Right", "Down", "Left"],
        "withJsonConverters": true
      }
    },
    "Model": {
      "Record": {
        "imports": [
          "Array exposing (Array)",
          "Gen.Enum.Direction exposing (Direction)"
        ],
        "fields": [
          { "name": "direction", "type": "Direction" },
          { "name": "todos", "type": "Array String" }
        ]
      }
    },
    "StringWriter": {
      "Writer": {
        "imports": [],
        "output": {
          "type": "String",
          "polymorphic": false,
          "empty": "\"\"",
          "append": "(\\c1 c2 -> c2 ++ c1)"
        }
      }
    },
    "CmdWriter": {
      "Writer": {
        "imports": [],
        "output": {
          "type": "Cmd",
          "polymorphic": true,
          "empty": "Cmd.none",
          "append": "(\\c1 c2 -> Cmd.batch [c1,c2])"
        }
      }
    }
  }
}
