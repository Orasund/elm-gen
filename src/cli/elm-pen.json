{
    "generateInto": "elm-pen/generated",
    "templatesFrom": "elm-pen/templates",
    "moduleBase": "Gen",
    "pathToElmFormat": "elm-format",
    "modules": {
      "Weekday": {
        "Enum": {
          "sorts": [
            "Monday",
            "Tuesday",
            "Wednesday",
            "Thursday",
            "Friday",
            "Saturday",
            "Sunday"
          ],
          "isCyclic": true,
          "withJsonConverters": true
        }
      },
      "Model": {
        "Record": {
          "imports": [
            "Array exposing (Array)",
            "Gen.Enum.Weekday as Weekday exposing (Weekday)"
          ],
          "fields": [
            {
              "name": "weekday",
              "type": "Weekday",
              "jsonDecoder": "Weekday.decoder",
              "jsonEncoder": "Weekday.encoder"
            },
            {
              "name": "todos",
              "type": "Array String",
              "jsonDecoder": "D.array D.string",
              "jsonEncoder": "E.array E.string"
            }
          ],
          "withJsonDecoder": true,
          "withJsonEncoder": true
        }
      },
      "StringWriter": {
        "Writer": {
          "imports": [],
          "output": {
            "type": "String",
            "polymorphic": false,
            "empty": "\"\"",
            "append": "(\\c1 c2 -> c2 ++ c1)"
          }
        }
      },
      "CmdWriter": {
        "Writer": {
          "imports": [],
          "output": {
            "type": "Cmd",
            "isPolymorphic": true,
            "empty": "Cmd.none",
            "append": "(\\c1 c2 -> Cmd.batch [c1,c2])"
          }
        }
      }
    }
  }
  